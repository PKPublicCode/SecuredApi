{
  "preRequestActions": [
  ],
  "onRequestErrorActions": [
  ],
  "onRequestSuccessActions": [
  ],
  "routesGroups": [
    {
      "description": "Welcome response",
      "routes": [
        {
          "routeKey": {
            "method": "get",
            "path": "/"
          },
          "actions": [
            {
              "type": "ReturnStaticFile",
              "settings": {
                "FileName": "StaticFiles/welcome.html"
              }
            }
          ]
        }
      ]
    }
  ],
  "routeNotFoundActions": [
    {
      "type": "SetResponse",
      "settings": {
        "httpCode": 501,
        "Body": "Route not configured"
      }
    }
  ]
}